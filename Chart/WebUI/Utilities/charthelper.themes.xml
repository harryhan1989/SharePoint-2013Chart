	<ChartTheme id="EarthTones">
		<Chart 
			Palette="EarthTones" 
			BackColor="Goldenrod" 
			BackGradientType="DiagonalLeft" 
			BackGradientEndColor="PaleGoldenrod" 
			BackHatchStyle="None"
			BorderLineColor="Gray" 
			BorderLineWidth="1"
			BorderLineStyle="NotSet" 
			BorderSkin-FrameBackColor="Khaki" 
			BorderSkin-FrameBackGradientEndColor="Olive" ></Chart>

		<ChartArea
			BackColor="?3D:120,Goldenrod; Default:Transparent"
			BorderColor="?3D:120,Goldenrod; Default:64,64,64"
			BorderStyle="?3D:Solid; Default:NotSet"
			BackGradientEndColor="White" 
			BackGradientType="None" 
			ShadowOffset="0" ></ChartArea>

		<Axis
			LineColor="?3D:120,Goldenrod; Default:64,64,64"
			LabelStyle-FontColor="Black" 
			MajorGrid-LineColor="Gray"
			MajorGrid-LineStyle="Dash" 
			MinorGrid-LineColor="127,185,185,185" 
			MinorGrid-LineStyle="Dash" ></Axis>

		<Legend
			BackColor = "Transparent"
			BorderColor = "Transparent" 
			ShadowColor = "100,Black"
			ShadowOffset = "2"
			FontColor = "White"
			></Legend>

		<Series
			ShadowOffset="?Area|SplineArea|StackedArea|100%StackedArea|Range|SplineRange:0; Default:2"
			BorderColor="64,64,64"
			BorderWidth = "1"			
			BackGradientEndColor="Khaki"
			BackGradientType="None"
			></Series>
	</ChartTheme>


